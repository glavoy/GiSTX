<?xml version = '1.0' encoding = 'utf-8'?>
<survey>


	<question type = 'automatic' fieldname = 'starttime' fieldtype = 'datetime'>
	</question>


	<question type = 'text' fieldname = 'subjid' fieldtype = 'text'>
		<text>SEARCH SAPPHIRE ID</text>
		<maxCharacters>10</maxCharacters>
	</question>


	<question type = 'radio' fieldname = 'withdrawreason' fieldtype = 'integer'>
		<text>Reason for subject withdrawal</text>
		<responses>
			<response value = '1'>Participant determined to be ineligible (select reason)</response>
			<response value = '2'>Withdrawal of informed consent</response>
			<response value = '3'>Study participant death</response>
			<response value = '4'>Other reason for withdrawal</response>
			<response value = '5'>Participant moved from the study community</response>
		</responses>
		<postskip>
			<skip fieldname='withdrawreason' condition = '&lt;&gt;' response='4' response_type='fixed' skiptofieldname ='ineligible'></skip>
		</postskip>
	</question>


	<question type = 'text' fieldname = 'orwithdraw' fieldtype = 'text'>
		<text>Specify other reason for withdrawal</text>
		<maxCharacters>80</maxCharacters>
		<postskip>
			<skip fieldname='withdrawreason' condition = '=' response='4' response_type='fixed' skiptofieldname ='withdrawdate'></skip>
		</postskip>
	</question>


	<question type = 'radio' fieldname = 'ineligible' fieldtype = 'integer'>
		<text>Whay was the participant determined to be ineligible</text>
		<responses>
			<response value = '1'>Negative HIV test</response>
			<response value = '2'>Other reason for ineligibility (specify)</response>
		</responses>
		<preskip>
			<skip fieldname='withdrawreason' condition = '&lt;&gt;' response='1' response_type='fixed' skiptofieldname ='withdrawdate'></skip>
		</preskip>
		<postskip>
			<skip fieldname='ineligible' condition = '=' response='1' response_type='fixed' skiptofieldname ='withdrawdate'></skip>
		</postskip>
	</question>


	<question type = 'text' fieldname = 'orineligible' fieldtype = 'text'>
		<text>Specify: Other reason for ineligibility</text>
		<maxCharacters>80</maxCharacters>
	</question>


	<question type = 'date' fieldname = 'withdrawdate' fieldtype = 'date'>
		<text>Withdrawal date

(if due to death, record death date)</text>
		<preskip>
			<skip fieldname='withdrawreason' condition = '=' response='5' response_type='fixed' skiptofieldname ='movedate_year'></skip>
		</preskip>
		<postskip>
			<skip fieldname='withdrawreason' condition = '&lt;&gt;' response='5' response_type='fixed' skiptofieldname ='comments'></skip>
		</postskip>
	</question>


	<question type = 'radio' fieldname = 'movedate_year' fieldtype = 'integer'>
		<text>MOVE DATE (year)</text>
		<responses>
			<response value = '2021'>2021</response>
			<response value = '2022'>2022</response>
			<response value = '2023'>2023</response>
		</responses>
		<dont_know>-7</dont_know>
	</question>


	<question type = 'radio' fieldname = 'movedate_month' fieldtype = 'integer'>
		<text>MOVE DATE (month)</text>
		<responses>
			<response value = '1'>January</response>
			<response value = '2'>February</response>
			<response value = '3'>March</response>
			<response value = '4'>April</response>
			<response value = '5'>May</response>
			<response value = '6'>June</response>
			<response value = '7'>July</response>
			<response value = '8'>August</response>
			<response value = '9'>September</response>
			<response value = '10'>October</response>
			<response value = '11'>November</response>
			<response value = '12'>December</response>
		</responses>
		<dont_know>-7</dont_know>
	</question>


	<question type = 'radio' fieldname = 'movedate_day' fieldtype = 'integer'>
		<text>MOVE DATE (day)</text>
		<logic_check>
			<logic fieldname='movedate_month' condition = '=' response = '2' response_type = 'fixed' condition2 = '=' currentresponse = '29' message = 'February does not have 29 days!'></logic>
			<logic fieldname='movedate_month' condition = '=' response = '2' response_type = 'fixed' condition2 = '=' currentresponse = '30' message = 'February does not have 30 days!'></logic>
			<logic fieldname='movedate_month' condition = '=' response = '2' response_type = 'fixed' condition2 = '=' currentresponse = '31' message = 'February does not have 31 days!'></logic>
			<logic fieldname='movedate_month' condition = '=' response = '4' response_type = 'fixed' condition2 = '=' currentresponse = '31' message = 'April only has 30 days!'></logic>
			<logic fieldname='movedate_month' condition = '=' response = '6' response_type = 'fixed' condition2 = '=' currentresponse = '31' message = 'June only has 30 days!'></logic>
			<logic fieldname='movedate_month' condition = '=' response = '9' response_type = 'fixed' condition2 = '=' currentresponse = '31' message = 'September only has 30 days!'></logic>
			<logic fieldname='movedate_month' condition = '=' response = '11' response_type = 'fixed' condition2 = '=' currentresponse = '31' message = 'November only has 30 days!'></logic>
		</logic_check>
		<responses>
			<response value = '1'>1</response>
			<response value = '2'>2</response>
			<response value = '3'>3</response>
			<response value = '4'>4</response>
			<response value = '5'>5</response>
			<response value = '6'>6</response>
			<response value = '7'>7</response>
			<response value = '8'>8</response>
			<response value = '9'>9</response>
			<response value = '10'>10</response>
			<response value = '11'>11</response>
			<response value = '12'>12</response>
			<response value = '13'>13</response>
			<response value = '14'>14</response>
			<response value = '15'>15</response>
			<response value = '16'>16</response>
			<response value = '17'>17</response>
			<response value = '18'>18</response>
			<response value = '19'>19</response>
			<response value = '20'>20</response>
			<response value = '21'>21</response>
			<response value = '22'>22</response>
			<response value = '23'>23</response>
			<response value = '24'>24</response>
			<response value = '25'>25</response>
			<response value = '26'>26</response>
			<response value = '27'>27</response>
			<response value = '28'>28</response>
			<response value = '29'>29</response>
			<response value = '30'>30</response>
			<response value = '31'>31</response>
		</responses>
		<dont_know>-7</dont_know>
	</question>


	<question type = 'text' fieldname = 'comments' fieldtype = 'text'>
		<text>Comments (optional)</text>
		<maxCharacters>80</maxCharacters>
	</question>


	<question type = 'automatic' fieldname = 'uniqueid' fieldtype = 'text'>
	</question>


	<question type = 'automatic' fieldname = 'swver' fieldtype = 'text'>
	</question>


	<question type = 'automatic' fieldname = 'lastmod' fieldtype = 'datetime'>
	</question>


	<question type = 'automatic' fieldname = 'stoptime' fieldtype = 'datetime'>
	</question>


	<question type = 'information' fieldname = 'end_of_questions' fieldtype = 'n/a'>
		<text>Press the 'Finish' button to save the data.</text >
	</question>


</survey>
