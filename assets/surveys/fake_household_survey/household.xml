<?xml version = '1.0' encoding = 'utf-8'?>
<survey>


	<question type = 'automatic' fieldname = 'starttime' fieldtype = 'datetime'>
	</question>


	<question type = 'radio' fieldname = 'community' fieldtype = 'integer'>
		<text>Community</text>
		<responses>
			<response value = '1'>Mbarara</response>
			<response value = '2'>Kampala</response>
			<response value = '3'>Jinja</response>
			<response value = '4'>Mbale</response>
		</responses>
	</question>


	<question type = 'automatic' fieldname = 'hhid' fieldtype = 'text'>
	</question>


	<question type = 'text' fieldname = 'headofhouseholdname' fieldtype = 'text'>
		<text>What is the name of the head of the household</text>
		<maxCharacters>80</maxCharacters>
	</question>


	<question type = 'text' fieldname = 'num_people' fieldtype = 'text_integer'>
		<text>How many people stay in this household?</text>
		<maxCharacters>2</maxCharacters>
		<numeric_check>
			<values minvalue ='1' maxvalue='50' other_values = '1' message = 'Number must be between 1 and 50!'></values>
		</numeric_check>
	</question>


	<question type = 'text' fieldname = 'comments' fieldtype = 'text'>
		<text>Comments (optional)</text>
		<maxCharacters>80</maxCharacters>
	</question>


	<question type = 'automatic' fieldname = 'uniqueid' fieldtype = 'text'>
	</question>


	<question type = 'automatic' fieldname = 'swver' fieldtype = 'text'>
	</question>


	<question type = 'automatic' fieldname = 'lastmod' fieldtype = 'datetime'>
	</question>


	<question type = 'automatic' fieldname = 'stoptime' fieldtype = 'datetime'>
	</question>


	<question type = 'information' fieldname = 'end_of_questions' fieldtype = 'n/a'>
		<text>Press the 'Finish' button to save the data.</text >
	</question>


</survey>
