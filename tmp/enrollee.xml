<?xml version = '1.0' encoding = 'utf-8'?>
<survey>


	<question type = 'automatic' fieldname = 'starttime' fieldtype = 'datetime'>
	</question>


	<question type = 'automatic' fieldname = 'startdate' fieldtype = 'date'>
	</question>


	<question type = 'text' fieldname = 'intnum' fieldtype = 'text_integer'>
		<text>Interviewer's Number</text>
		<maxCharacters>=2</maxCharacters>
		<numeric_check>
			<values minvalue ='10' maxvalue='99' other_values = '10' message = 'Number must be between 10 and 99!'></values>
		</numeric_check>
	</question>


	<question type = 'radio' fieldname = 'mrc' fieldtype = 'integer'>
		<text>MRC of enrollment</text>
		<responses>
			<response value = '1'>Busitema HC III</response>
			<response value = '2'>Kigandalo HC IV</response>
		</responses>
	</question>


	<question type = 'automatic' fieldname = 'mrc_code' fieldtype = 'text'>
		<calculation type='case'>
			<when field='mrc' operator='=' value='1'>
				<result type='constant' value='096' />
			</when>
			<else>
				<result type='constant' value='110' />
			</else>
		</calculation>
	</question>


	<question type = 'automatic' fieldname = 'subjid' fieldtype = 'text'>
	</question>


	<question type = 'radio' fieldname = 'district' fieldtype = 'integer'>
		<text>District of residence of enrollee</text>
		<responses>
			<response value = '124'>BUSIA</response>
			<response value = '181'>MAYUGE</response>
			<response value = '999'>Does not reside in either district</response>
		</responses>
		<postskip>
			<skip fieldname='district' condition = '=' response='999' response_type='fixed' skiptofieldname ='placeofresidence'></skip>
		</postskip>
	</question>


	<question type = 'radio' fieldname = 'mayuge_warning' fieldtype = 'integer'>
		<text>WARNING!

You selected Busitema HC III and Mayuge as the district of residence!

Do you want to proceed anyway?</text>
		<responses>
			<response value = '1'>Yes - I am at Busitema HC III and this participant resides in Mayuge district.</response>
		</responses>
		<preskip>
			<skip fieldname='mrc' condition = '=' response='110' response_type='fixed' skiptofieldname ='busia_warning'></skip>
			<skip fieldname='district' condition = '=' response='124' response_type='fixed' skiptofieldname ='subcounty'></skip>
		</preskip>
	</question>


	<question type = 'radio' fieldname = 'busia_warning' fieldtype = 'integer'>
		<text>WARNING!

You selected Kigandalo HC IV and Busia as the district of residence!

Do you want to proceed anyway?</text>
		<responses>
			<response value = '1'>Yes - I am at Kigandalo HC IV and this participant resides in Busia district.</response>
		</responses>
		<preskip>
			<skip fieldname='mrc' condition = '=' response='096' response_type='fixed' skiptofieldname ='subcounty'></skip>
			<skip fieldname='district' condition = '=' response='181' response_type='fixed' skiptofieldname ='subcounty'></skip>
		</preskip>
	</question>


	<question type = 'radio' fieldname = 'subcounty' fieldtype = 'integer'>
		<text>Subcounty of residence of enrollee</text>
		<maxCharacters>=2</maxCharacters>
		<responses source='csv' file='villages.csv'>
			<filter column='districtid' operator='=' value='[[district]]'/>
			<display column='subcounty'/>
			<value column='subcountyid'/>
		</responses>
	</question>


	<question type = 'radio' fieldname = 'parish' fieldtype = 'integer'>
		<text>Parish of residence of enrollee</text>
		<maxCharacters>=2</maxCharacters>
		<responses source='csv' file='villages.csv'>
			<filter column='districtid' operator='=' value='[[district]]'/>
			<filter column='subcountyid' operator='=' value='[[subcounty]]'/>
			<display column='parish'/>
			<value column='parishid'/>
		</responses>
	</question>


	<question type = 'radio' fieldname = 'village' fieldtype = 'integer'>
		<text>Village of residence of enrollee</text>
		<maxCharacters>=2</maxCharacters>
		<responses source='csv' file='villages.csv'>
			<filter column='districtid' operator='=' value='[[district]]'/>
			<filter column='subcountyid' operator='=' value='[[subcounty]]'/>
			<filter column='parishid' operator='=' value='[[parish]]'/>
			<display column='village'/>
			<value column='villageid'/>
		</responses>
	</question>


	<question type = 'text' fieldname = 'placeofresidence' fieldtype = 'text'>
		<text>Enter the participants district, subcounty, parish and village of residence</text>
		<maxCharacters>255</maxCharacters>
		<preskip>
			<skip fieldname='district' condition = '&lt;&gt;' response='999' response_type='fixed' skiptofieldname ='dob'></skip>
		</preskip>
	</question>


	<question type = 'date' fieldname = 'dob' fieldtype = 'date'>
		<text>Record date of birth

If the exact day of month is unknown put 15th.

If the exact month is unknown put the first month of the season of the year when they were born</text>
		<date_range>
			<min_date>-3y</min_date>
			<max_date>-4m</max_date>
		</date_range>
	</question>


	<question type = 'automatic' fieldname = 'age_calculated' fieldtype = 'integer'>
		<calculation type='age_from_date' field='dob' value='years'/>
	</question>


	<question type = 'automatic' fieldname = 'agemonths_calculated' fieldtype = 'integer'>
		<calculation type='age_from_date' field='dob' value='months'/>
	</question>


	<question type = 'automatic' fieldname = 'age_at_apr2025' fieldtype = 'integer'>
		<calculation type='age_at_date' field='dob' value='months' separator='2025-04-01'/>
	</question>


	<question type = 'automatic' fieldname = 'age_in_range' fieldtype = 'integer'>
		<calculation type='case'>
			<when field='agemonths_calculated' operator='&lt;' value='8'>
				<result type='constant' value='0' />
			</when>
			<when field='age_at_apr2025' operator='&gt;=' value='12'>
				<result type='constant' value='0' />
			</when>
			<else>
				<result type='constant' value='1' />
			</else>
		</calculation>
	</question>


	<question type = 'text' fieldname = 'age' fieldtype = 'integer'>
		<text>Record age in years.

Enter 0 if child is under 1 year old</text>
		<maxCharacters>1</maxCharacters>
		<numeric_check>
			<values minvalue ='0' maxvalue='5' other_values = '0' message = 'Number must be between 0 and 5!'></values>
		</numeric_check>
		<logic_check>
			age &lt;&gt; age_calculated; 'The age entered does not match the date of birth!'
		</logic_check>
	</question>


	<question type = 'text' fieldname = 'agemonths' fieldtype = 'integer'>
		<text>Record age in months</text>
		<numeric_check>
			<values minvalue ='0' maxvalue='11' other_values = '0' message = 'Number must be between 0 and 11!'></values>
		</numeric_check>
		<logic_check>
			agemonths_calculated &lt;&gt; agemonths; 'Age in months does not match the age in months from the DOB!'
		</logic_check>
		<preskip>
			<skip fieldname='age' condition = '&lt;&gt;' response='0' response_type='fixed' skiptofieldname ='age_eligible'></skip>
		</preskip>
	</question>


	<question type = 'radio' fieldname = 'age_eligible' fieldtype = 'integer'>
		<text>Age eligibility:

Age in Years: [[age]]
Age in Months: [[agemonths_calculated]]

Age in months on April 1 2025: [[age_at_apr2025]]

Individual is 8 months or older at time of enrollment (today) AND was younger than 1 year (12 months) on April 1st, 2025</text>
		<logic_check>
			age_in_range = 1 and age_eligible = 0; 'Participant is within the age range and must be marked as age eligible!'
		</logic_check>
		<responses>
			<response value = '1'>Yes - continue to next criterium</response>
			<response value = '0'>No - exclude</response>
		</responses>
	</question>


	<question type = 'radio' fieldname = 'age_warning' fieldtype = 'integer'>
		<text>WARNING!

The child's age is outside the protocol enrollment range (8 months to 22+ months shifting limit).

Years: [[age]]
Months: [[agemonths]]

Do you want to proceed with enrollment anyway?</text>
		<responses>
			<response value = '1'>Yes - proceed anyway</response>
			<response value = '0'>No - exclude</response>
		</responses>
		<preskip>
			<skip fieldname='age_eligible' condition = '=' response='0' response_type='fixed' skiptofieldname ='mal_test_eligible'></skip>
			<skip fieldname='age_in_range' condition = '=' response='1' response_type='fixed' skiptofieldname ='mal_test_eligible'></skip>
		</preskip>
		<postskip>
			<skip fieldname='age_warning' condition = '=' response='0' response_type='fixed' skiptofieldname ='comments'></skip>
		</postskip>
	</question>


	<question type = 'text' fieldname = 'age_warning_reason' fieldtype = 'text'>
		<text>Please provide a reason for enrolling this participant outside the protocol age range.</text>
		<maxCharacters>80</maxCharacters>
	</question>


	<question type = 'radio' fieldname = 'mal_test_eligible' fieldtype = 'integer'>
		<text>RDT or microscopy conducted (result can be positive or negative)</text>
		<responses>
			<response value = '1'>Yes - continue to next criterium</response>
			<response value = '0'>No - exclude</response>
		</responses>
		<preskip>
			<skip fieldname='age_eligible' condition = '=' response='0' response_type='fixed' skiptofieldname ='comments'></skip>
		</preskip>
	</question>


	<question type = 'radio' fieldname = 'consent_eligible' fieldtype = 'integer'>
		<text>Parent/guardian willing to provide consent</text>
		<responses>
			<response value = '1'>Yes - include, begin survey</response>
			<response value = '0'>No - exclude</response>
		</responses>
		<preskip>
			<skip fieldname='mal_test_eligible' condition = '=' response='0' response_type='fixed' skiptofieldname ='comments'></skip>
		</preskip>
	</question>


	<question type = 'text' fieldname = 'participantsname' fieldtype = 'text'>
		<text>Participant names?</text>
		<maxCharacters>80</maxCharacters>
		<preskip>
			<skip fieldname='consent_eligible' condition = '=' response='0' response_type='fixed' skiptofieldname ='comments'></skip>
		</preskip>
	</question>


	<question type = 'radio' fieldname = 'gender' fieldtype = 'integer'>
		<text>Gender of [[participantsname]]?</text>
		<responses>
			<response value = '1'>Male</response>
			<response value = '2'>Female</response>
		</responses>
	</question>


	<question type = 'radio' fieldname = 'hhheadeduclevel' fieldtype = 'integer'>
		<text>What is education level of the household head of the house the participant lives in?</text>
		<responses>
			<response value = '0'>No education/Never gone to school</response>
			<response value = '1'>Nursery</response>
			<response value = '2'>Primary</response>
			<response value = '3'>Secondary</response>
			<response value = '4'>Vocational</response>
			<response value = '5'>University and Above</response>
		</responses>
	</question>


	<question type = 'radio' fieldname = 'diagnostic' fieldtype = 'integer'>
		<text>What diagnostic test was used?</text>
		<responses>
			<response value = '0'>Microscopy</response>
			<response value = '1'>RDT</response>
		</responses>
	</question>


	<question type = 'radio' fieldname = 'result' fieldtype = 'integer'>
		<text>What is the results of the diagnostic?</text>
		<responses>
			<response value = '0'>Negative</response>
			<response value = '1'>Positive</response>
			<response value = '3'>Indeterminate</response>
		</responses>
	</question>


	<question type = 'radio' fieldname = 'vx_card' fieldtype = 'integer'>
		<text>Does the parent or guardian have a vaccine card for [[participantsname]]?</text>
		<responses>
			<response value = '1'>Yes</response>
			<response value = '0'>No</response>
		</responses>
	</question>


	<question type = 'radio' fieldname = 'vx_any' fieldtype = 'integer'>
		<text>Has [[participantsname]] received any doses of the malaria vaccine?</text>
		<responses>
			<response value = '1'>Yes</response>
			<response value = '0'>No</response>
		</responses>
		<preskip>
			<skip fieldname='age' condition = '&gt;' response='4' response_type='fixed' skiptofieldname ='hib_any'></skip>
		</preskip>
	</question>


	<question type = 'radio' fieldname = 'vx_any_no' fieldtype = 'integer'>
		<text>Why did [[participantsname]] not received the malaria vaccine?</text>
		<responses>
			<response value = '1'>Parent reports child ineligible</response>
			<response value = '2'>Vaccine not available</response>
			<response value = '3'>Could not travel to clinic</response>
			<response value = '4'>Refused/Did not want</response>
			<response value = '96'>Other</response>
		</responses>
		<preskip>
			<skip fieldname='vx_any' condition = '=' response='1' response_type='fixed' skiptofieldname ='vx_doses_received'></skip>
		</preskip>
	</question>


	<question type = 'text' fieldname = 'vx_any_no_oth' fieldtype = 'text'>
		<text>Specify other:</text>
		<maxCharacters>80</maxCharacters>
		<preskip>
			<skip fieldname='vx_any_no' condition = '&lt;&gt;' response='96' response_type='fixed' skiptofieldname ='hib_any'></skip>
		</preskip>
	</question>


	<question type = 'text' fieldname = 'vx_doses_received' fieldtype = 'text_integer'>
		<text>As of today's date, how many doses of malaria vaccine has [[participantsname]] received?</text>
		<maxCharacters>1</maxCharacters>
		<numeric_check>
			<values minvalue ='1' maxvalue='4' other_values = '1' message = 'Number must be between 1 and 4!'></values>
		</numeric_check>
		<preskip>
			<skip fieldname='vx_any' condition = '=' response='0' response_type='fixed' skiptofieldname ='hib_any'></skip>
		</preskip>
	</question>


	<question type = 'radio' fieldname = 'vx_doses_received_ver' fieldtype = 'integer'>
		<text>Method of verification of number of doses (Select one):</text>
		<logic_check>
			vx_card = 0 and vx_doses_received_ver = 2; 'You previously indicated that the guardian does not have a vaccine card!'
		</logic_check>
		<responses>
			<response value = '1'>Guardian report</response>
			<response value = '2'>Vaccine card</response>
			<response value = '96'>Other</response>
		</responses>
	</question>


	<question type = 'text' fieldname = 'vx_doses_received_ver_oth' fieldtype = 'text'>
		<text>Specify other:</text>
		<maxCharacters>80</maxCharacters>
		<preskip>
			<skip fieldname='vx_doses_received_ver' condition = '&lt;&gt;' response='96' response_type='fixed' skiptofieldname ='vx_dose1_where'></skip>
		</preskip>
	</question>


	<question type = 'radio' fieldname = 'vx_dose1_where' fieldtype = 'integer'>
		<text>Where was dose 1 received? (Select one)</text>
		<responses>
			<response value = '1'>Health post or health center</response>
			<response value = '2'>Community/Village location</response>
			<response value = '96'>Other</response>
		</responses>
	</question>


	<question type = 'text' fieldname = 'vx_dose1_where_oth' fieldtype = 'text'>
		<text>Specify other:</text>
		<maxCharacters>80</maxCharacters>
		<preskip>
			<skip fieldname='vx_dose1_where' condition = '&lt;&gt;' response='96' response_type='fixed' skiptofieldname ='vx_dose1_date'></skip>
		</preskip>
	</question>


	<question type = 'date' fieldname = 'vx_dose1_date' fieldtype = 'date'>
		<text>On what date was dose 1 of the malaria vaccine received?</text>
		<date_range>
			<min_date>2025-03-01</min_date>
			<max_date>0</max_date>
		</date_range>
		<logic_check>
			vx_dose1_date &lt; dob; 'Date of vaccination cannot be before date of birth!'
		</logic_check>
	</question>


	<question type = 'radio' fieldname = 'vx_dose1_date_ver' fieldtype = 'integer'>
		<text>Method of verification of dose 1 date</text>
		<logic_check>
			vx_card = 0 and vx_dose1_date_ver = 2; 'You previously indicated that the guardian does not have a vaccine card!'
		</logic_check>
		<responses>
			<response value = '1'>Guardian report</response>
			<response value = '2'>Vaccine card</response>
			<response value = '96'>Other</response>
		</responses>
		<postskip>
			<skip fieldname='vx_dose1_date_ver' condition = '&lt;&gt;' response='96' response_type='fixed' skiptofieldname ='vx_dose2_where'></skip>
		</postskip>
	</question>


	<question type = 'text' fieldname = 'vx_dose1_date_ver_oth' fieldtype = 'text'>
		<text>Specify other:</text>
		<maxCharacters>80</maxCharacters>
	</question>


	<question type = 'radio' fieldname = 'vx_dose2_where' fieldtype = 'integer'>
		<text>Where was dose 2 received?</text>
		<responses>
			<response value = '1'>Health post or health center</response>
			<response value = '2'>Community/Village location</response>
			<response value = '96'>Other</response>
		</responses>
		<preskip>
			<skip fieldname='vx_doses_received' condition = '&lt;' response='2' response_type='fixed' skiptofieldname ='vx_dose_summary'></skip>
		</preskip>
		<postskip>
			<skip fieldname='vx_dose2_where' condition = '&lt;&gt;' response='96' response_type='fixed' skiptofieldname ='vx_dose2_date'></skip>
		</postskip>
	</question>


	<question type = 'text' fieldname = 'vx_dose2_where_oth' fieldtype = 'text'>
		<text>Specify other:</text>
		<maxCharacters>80</maxCharacters>
	</question>


	<question type = 'date' fieldname = 'vx_dose2_date' fieldtype = 'date'>
		<text>On what date was dose 2 of the malaria vaccine received?</text>
		<date_range>
			<min_date>2025-03-01</min_date>
			<max_date>0</max_date>
		</date_range>
		<logic_check>
			vx_dose2_date &lt;= vx_dose1_date; 'Date of dose 2 cannot be on or before date of dose 1'
		</logic_check>
		<logic_check>
			vx_dose2_date &lt; dob; 'Date of vaccination cannot be before date of birth!'
		</logic_check>
	</question>


	<question type = 'automatic' fieldname = 'dose2_warning_time' fieldtype = 'integer'>
		<calculation type='date_diff' field='vx_dose1_date' value='vx_dose2_date' unit='w' />
	</question>


	<question type = 'radio' fieldname = 'vx_dose2_datecheck' fieldtype = 'integer'>
		<text>WARNING!

Dose 2 is less than 3 weeks after dose 1. Is this correct?
Otherwise go back and correct the dose dates</text>
		<responses>
			<response value = '1'>Yes - proceed</response>
		</responses>
		<preskip>
			<skip fieldname='dose2_warning_time' condition = '&gt;=' response='3' response_type='fixed' skiptofieldname ='vx_dose2_date_ver'></skip>
		</preskip>
	</question>


	<question type = 'radio' fieldname = 'vx_dose2_date_ver' fieldtype = 'integer'>
		<text>Method of verification of dose 2 date</text>
		<logic_check>
			vx_card = 0 and vx_dose2_date_ver = 2; 'You previously indicated that the guardian does not have a vaccine card!'
		</logic_check>
		<responses>
			<response value = '1'>Guardian report</response>
			<response value = '2'>Vaccine card</response>
			<response value = '96'>Other</response>
		</responses>
		<postskip>
			<skip fieldname='vx_dose2_date_ver' condition = '&lt;&gt;' response='96' response_type='fixed' skiptofieldname ='vx_dose3_where'></skip>
		</postskip>
	</question>


	<question type = 'text' fieldname = 'vx_dose2_date_ver_oth' fieldtype = 'text'>
		<text>Specify other:</text>
		<maxCharacters>80</maxCharacters>
	</question>


	<question type = 'radio' fieldname = 'vx_dose3_where' fieldtype = 'integer'>
		<text>Where was dose 3 received?</text>
		<responses>
			<response value = '1'>Health post or health center</response>
			<response value = '2'>Community/Village location</response>
			<response value = '96'>Other</response>
		</responses>
		<preskip>
			<skip fieldname='vx_doses_received' condition = '&lt;' response='3' response_type='fixed' skiptofieldname ='vx_dose_summary'></skip>
		</preskip>
		<postskip>
			<skip fieldname='vx_dose3_where' condition = '&lt;&gt;' response='96' response_type='fixed' skiptofieldname ='vx_dose3_date'></skip>
		</postskip>
	</question>


	<question type = 'text' fieldname = 'vx_dose3_where_oth' fieldtype = 'text'>
		<text>Specify other:</text>
		<maxCharacters>80</maxCharacters>
	</question>


	<question type = 'date' fieldname = 'vx_dose3_date' fieldtype = 'date'>
		<text>On what date was dose 3 of the malaria vaccine received?</text>
		<date_range>
			<min_date>2025-03-01</min_date>
			<max_date>0</max_date>
		</date_range>
		<logic_check>
			vx_dose3_date &lt;= vx_dose2_date; 'Date of dose 3 cannot be on or before date of dose 2'
		</logic_check>
		<logic_check>
			vx_dose3_date &lt; dob; 'Date of vaccination cannot be before date of birth!'
		</logic_check>
	</question>


	<question type = 'automatic' fieldname = 'dose3_warning_time' fieldtype = 'integer'>
		<calculation type='date_diff' field='vx_dose2_date' value='vx_dose3_date' unit='w' />
	</question>


	<question type = 'radio' fieldname = 'vx_dose3_datecheck' fieldtype = 'integer'>
		<text>WARNING!

Dose 3 is less than 4 weeks after dose 2. Is this correct?
Otherwise go back and correct the dose dates</text>
		<responses>
			<response value = '1'>Yes - proceed</response>
		</responses>
		<preskip>
			<skip fieldname='dose3_warning_time' condition = '&gt;=' response='4' response_type='fixed' skiptofieldname ='vx_dose3_date_ver'></skip>
		</preskip>
	</question>


	<question type = 'radio' fieldname = 'vx_dose3_date_ver' fieldtype = 'integer'>
		<text>Method of verification of dose 3 date</text>
		<logic_check>
			vx_card = 0 and vx_dose3_date_ver = 2; 'You previously indicated that the guardian does not have a vaccine card!'
		</logic_check>
		<responses>
			<response value = '1'>Guardian report</response>
			<response value = '2'>Vaccine card</response>
			<response value = '96'>Other</response>
		</responses>
		<postskip>
			<skip fieldname='vx_dose3_date_ver' condition = '&lt;&gt;' response='96' response_type='fixed' skiptofieldname ='vx_dose4_where'></skip>
		</postskip>
	</question>


	<question type = 'text' fieldname = 'vx_dose3_date_ver_oth' fieldtype = 'text'>
		<text>Specify other:</text>
		<maxCharacters>80</maxCharacters>
	</question>


	<question type = 'radio' fieldname = 'vx_dose4_where' fieldtype = 'integer'>
		<text>Where was dose 4 received?</text>
		<responses>
			<response value = '1'>Health post or health center</response>
			<response value = '2'>Community/Village location</response>
			<response value = '96'>Other</response>
		</responses>
		<preskip>
			<skip fieldname='vx_doses_received' condition = '&lt;' response='4' response_type='fixed' skiptofieldname ='vx_dose_summary'></skip>
		</preskip>
		<postskip>
